{
    "manifest_version": 2,

    "name": "Salesforce1 Launcher",
    "description": "Launch Salesforce1 in a popup window.",
    "version": "0.0.1",
	
    "background": {
        "scripts": ["background.js"]
    },

    "permissions": ["tabs"],

    "browser_action": {
        "default_icon": "salesforce1.png"
    },

    "icons": {
        "16": "salesforce1.png",
        "48": "salesforce1.png",
        "128": "salesforce1.png"
    }
}